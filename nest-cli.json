{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ytravel-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ytravel-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ytravel-backend",
  "projects": {
    "ytravel-backend": {
      "type": "application",
      "root": "apps/ytravel-backend",
      "entryFile": "main",
      "sourceRoot": "apps/ytravel-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ytravel-backend/tsconfig.app.json"
      }
    },
    "smtp": {
      "type": "application",
      "root": "apps/smtp",
      "entryFile": "main",
      "sourceRoot": "apps/smtp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/smtp/tsconfig.app.json"
      }
    }
  }
}